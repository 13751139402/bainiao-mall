view {
  /*使用 主题颜色 *//*: var(--themeColor);*/
}

.index_swiper swiper {
  width: 750rpx;
  height: 340rpx;
}

.index_swiper swiper image {
  width: 100%;
}

.index_cate {
  display: flex;
}

.index_cate navigator {
  flex: 1;
}

.index_cate navigator image {
  padding: 20rpx;
  width: 100%;
}

.floor_title image {
  padding: 10rpx 0;
  width: 100%;
}
.floor_list{
  overflow: hidden;
}
.floor_list navigator {
  float: left;
  width: 33.33%;
}

.floor_list navigator:nth-last-child(-n+4) image {
  height: 27.72711207vw;
  border-left:10rpx solid #fff;
}
.floor_list navigator:nth-last-child(1) image
,.floor_list navigator:nth-last-child(2) image{
  border-bottom: 10rpx solid #fff;
}

.floor_list  image {
  width: 100%;
  height: 100%;
}
